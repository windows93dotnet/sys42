{
  name: "TextEdit",
  command: "textedit",
  categories: ["utilities", "productivity", "development"],
  description: "Simple text editor using textarea",

  multiple: true,
  empty: false,
  width: 300,
  height: 300,

  decode: {
    types: [
      {
        description: "Text",
        accept: [
          "text/*",
          "application/json",
          "application/json5",
          "application/rss+xml",
          "application/xml",
        ],
      },
    ],
  },

  preload: ["../../../ui/components/tabs.js"],

  $ref: "./app.js",
}
