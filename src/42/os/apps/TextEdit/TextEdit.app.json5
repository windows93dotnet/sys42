{
  name: "TextEdit",
  command: "textedit",
  categories: ["utilities", "productivity", "development"],
  description: "A simple text editor using textarea",

  width: 300,
  height: 300,

  decode: {
    types: [
      {
        description: "Text",
        accept: [
          "text/*",
          "application/json",
          "application/rss+xml",
          "application/xml",
        ],
      },
    ],
  },

  state: {
    monospace: true,
    spellcheck: false,
    wrap: true,
  },

  $ref: "./TextEdit.js",
}
